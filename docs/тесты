запрос снимка с именем point
echo -n -e '\x01\x70\x6f\x69\x6e\x74' >/dev/tcp/127.0.0.1/30001
echo -n -e '\x01\x70\x6f\x69\x6e\x74' >/dev/tcp/10.11.86.249/5000

запрос старта записи видео с именем point
